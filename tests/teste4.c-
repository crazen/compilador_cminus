void main(void) {
    int a;
    int b;
    int c;
    
    /* Deve ser 7 (2*3=6, +1=7), nao 9 */
    a = 1 + 2 * 3;
    
    /* Deve ser 8 (4/2=2, 10-2=8), nao 3 */
    b = 10 - 4 / 2;
    
    /* Parenteses forcam prioridade: (1+2)=3, *3=9 */
    c = (1 + 2) * 3;
    
    output(a);
    output(b);
    output(c);
}